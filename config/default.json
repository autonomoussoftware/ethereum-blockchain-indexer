{
  "enode": {
    "ipcPath": "~/.local/share/io.parity.ethereum/jsonrpc.ipc",
    "wsPath": "ws://localhost:8546"
  },
  "events": {
    "maxAddresses": 10
  },
  "logger": {
    "Console": {
      "colorize": true,
      "level": "debug",
      "prettyPrint": true,
      "stringify": true
    },
    "Papertrail": {
      "colorize": true,
      "flushOnClose": true,
      "handleExceptions": true,
      "inlineMeta": true,
      "level": "info"
    }
  },
  "maxReorgWindow": 28800,
  "mongo": {
    "url": "mongodb://localhost:27017",
    "dbName": "indexer"
  },
  "newRelic": {},
  "pauseOnError": 2000,
  "port": 3005,
  "pubsub": "redis",
  "redis": {
    "url": "redis://localhost:6379"
  },
  "syncTimerSec": 1000
}
